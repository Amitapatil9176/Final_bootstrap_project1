        function base64Encode(data) {
            const jsonString = JSON.stringify(data);
            const encoded = encodeURIComponent(jsonString).replace(/%([0-9A-F]{2})/g, function(match, p1) {
                return String.fromCharCode('0x' + p1);
            });
            return btoa(encoded);
        }   
